<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.itzb.paintdemo">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/Theme.AppCompat.Light.NoActionBar">
		<activity android:name=".path.dragBubble.DragBubbleActivity"></activity>
		<activity android:name=".path.simple.PathActivity" />
		<activity android:name=".canvas.splash.SplashActivity" />
		<activity android:name=".canvas.split.SplitActivity" />
		<activity android:name=".canvas.transform.TransformActivity" />
		<activity android:name=".paint.colorfilter.ColorFilterActivity" />
		<activity android:name=".paint.xfermode.XfermodesActivity" />
		<activity android:name=".paint.simple.PaintActivity" />
		<activity android:name=".MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	</application>

</manifest>